{
   "name": "vite2-vue3-typescript-scss-starter",
   "version": "0.0.1",
   "description": "vite2 starter app",
   "scripts": {
      "dev": "vite-ssr --open",
      "build": "vite-ssr build && bash ./build.sh && npm run push:heroku",
      "build:spa": "vite build",
      "test:e2e": "start-server-and-test dev http-get://localhost:3000 cypress",
      "cypress": "cypress run --browser edge --no-exit",
      "push:heroku": "cd ./_app && git add . && git commit -m \"qeqwadas\" && git push heroku master"
   },
   "dependencies": {
      "@nuxt/devalue": "^2.0.0",
      "@vue/runtime-core": "^3.1.3",
      "@vue/server-renderer": "^3.1.4",
      "@vueuse/head": "^0.6.0",
      "markdown-it-prism": "^2.1.8",
      "node-fetch": "^2.6.1",
      "pinia": "^2.0.0-beta.5",
      "ts-node": "^10.1.0",
      "typescript": "^4.1.3",
      "vite-plugin-md": "^0.8.0",
      "vite-ssr": "^0.10.6",
      "vue": "3.1.4",
      "vue-router": "next",
      "windicss": "^3.1.5"
   },
   "devDependencies": {
      "@cypress/vite-dev-server": "^2.0.1",
      "@types/express": "^4.17.13",
      "@types/jest": "^26.0.23",
      "@types/node": "^15.6.0",
      "@types/node-fetch": "^2.5.11",
      "@vitejs/plugin-legacy": "^1.4.3",
      "@vitejs/plugin-vue": "latest",
      "@vitejs/plugin-vue-jsx": "^1.1.6",
      "@vue/compiler-sfc": "latest",
      "autoprefixer": "^10.2.5",
      "cross-env": "^7.0.3",
      "cypress": "^7.5.0",
      "sass": "latest",
      "start-server-and-test": "^1.12.5",
      "vite": "^2.4.2",
      "vite-plugin-windicss": "^1.2.4"
   }
}
